<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<!-- 
    Declaration of Converters
-->

<beans>
    
    <!-- all Converters used by the connector framework -->
    <bean id="DateToStringConverter" class="at.workflow.webdesk.po.impl.converter.DateToStringConverter"/>
    <bean id="NumberToStringConverter" class="at.workflow.webdesk.po.impl.converter.NumberToStringConverter"/>
    <bean id="StringToDateConverter" class="at.workflow.webdesk.po.impl.converter.StringToDateConverter"/>
    <bean id="StringToDoubleConverter" class="at.workflow.webdesk.po.impl.converter.StringToDoubleConverter"/>
    <bean id="StringToIntegerConverter" class="at.workflow.webdesk.po.impl.converter.StringToIntegerConverter"/>
    
</beans>