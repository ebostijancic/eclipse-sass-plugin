<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class lazy="true" table="PoRoleHolder" name="at.workflow.webdesk.po.model.PoRoleCompetenceBase">
    <cache usage="read-write" />
    <id name="UID" type="string" column="ROLEHOLDER_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <discriminator type="string" column="type" />
    <many-to-one column="ROLE_UID" cascade="none" foreign-key="FK_ROLEHOLDER_ROLE" not-null="true" name="role" />
    <bag inverse="true" cascade="all-delete-orphan" lazy="true" name="roleHolderPersons">
      <key not-null="false" column="ROLEHOLDER_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoRoleHolderPerson" />
    </bag>
    <bag inverse="true" cascade="all-delete-orphan" lazy="true" name="roleHolderGroups">
      <key not-null="false" column="ROLEHOLDER_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoRoleHolderGroup" />
    </bag>
    <bag inverse="true" cascade="all-delete-orphan" lazy="true" name="roleHolderDynamics">
      <key not-null="false" column="ROLEHOLDER_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoRoleHolderDynamic" />
    </bag>
    <property name="validfrom" not-null="true" type="timestamp" />
    <property name="validto" not-null="true" type="timestamp" />
    <subclass name="at.workflow.webdesk.po.model.PoRoleCompetenceAll" discriminator-value="CA" />
    <subclass name="at.workflow.webdesk.po.model.PoRoleCompetenceClient" discriminator-value="CC">
      <many-to-one column="CLIENT_UID" cascade="none" foreign-key="FK_ROLEHOLDER_CLIENT_CMPET" lazy="false" not-null="false" name="competence4Client" />
    </subclass>
    <subclass name="at.workflow.webdesk.po.model.PoRoleCompetenceGroup" discriminator-value="CG">
      <many-to-one column="GROUP_UID" cascade="none" foreign-key="FK_ROLEHOLDER_GROUP_CMPET" lazy="false" not-null="false" name="competence4Group" />
      <property name="inheritToChilds" />
    </subclass>
    <subclass name="at.workflow.webdesk.po.model.PoRoleCompetencePerson" discriminator-value="CP">
      <many-to-one column="PERSON_UID" cascade="none" foreign-key="FK_ROLEHOLDER_PERSON_CMPET" lazy="false" not-null="false" name="competence4Person" />
    </subclass>
  </class>
</hibernate-mapping>
