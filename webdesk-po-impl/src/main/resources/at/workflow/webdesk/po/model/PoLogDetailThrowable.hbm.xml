<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoLogDetailThrowable" name="at.workflow.webdesk.po.model.PoLogDetailThrowable">
    <id name="UID" type="string" column="LOGDETAILTHROWABLE_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="message" length="2000" />
    <property name="position" column="pos" />
    <many-to-one column="LOGDETAIL_UID" cascade="none" foreign-key="FK_THROWABLE_LOGDETAIL" not-null="false" name="logDetail" />
  </class>
</hibernate-mapping>
