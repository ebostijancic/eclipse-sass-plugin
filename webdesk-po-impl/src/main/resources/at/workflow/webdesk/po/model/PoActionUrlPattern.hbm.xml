<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class lazy="true" table="PoActionUrlPattern" name="at.workflow.webdesk.po.model.PoActionUrlPattern">
    <cache usage="read-write" />
    <id name="UID" type="string" column="ACTIONURLPATTERN_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <many-to-one column="ACTION_UID" cascade="none" foreign-key="FK_ACTIONURLPATTERN_ACTION" 
        lazy="false" not-null="true" name="action" />
    <property name="name" not-null="true" />
    <property name="pattern" not-null="true"/>
  </class>
</hibernate-mapping>
