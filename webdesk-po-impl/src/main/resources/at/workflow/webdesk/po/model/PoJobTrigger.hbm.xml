<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoJobTrigger" name="at.workflow.webdesk.po.model.PoJobTrigger">
    <cache usage="read-write" />
    <id name="UID" type="string" column="JOBTRIGGER_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="runInCluster" />
    <property name="name" not-null="true" />
    <many-to-one column="JOB_UID" cascade="none" foreign-key="FK_JOBTRIGGER_JOB" not-null="false" name="job" />
    <property name="cronExpression" />
    <property name="scheduleType" />
    <property name="intervalMinutes" />
    <property name="startDate" type="timestamp" />
    <property name="endDate" type="timestamp" />
    <property name="active" />
    <property name="repeatCount" />
    <!--property name="group" column="GROUPNAME" /-->
  </class>
</hibernate-mapping>
