<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class lazy="true" table="PoClient" name="at.workflow.webdesk.po.model.PoClient">
    <cache usage="read-write" />
    <id name="UID" type="string" column="CLIENT_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="name" unique="true" />
    <property name="description" />
    <property name="groupShortNamePrefix" />
    <property name="personUserNamePrefix" />
    <property name="personEmployeeIdPrefix" />
    <set inverse="true" cascade="save-update" lazy="true" name="roles">
      <key column="CLIENT_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoRole" />
    </set>
    <set inverse="true" cascade="save-update" lazy="true" name="persons">
      <key column="CLIENT_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoPerson" />
    </set>
    <set inverse="true" cascade="save-update" lazy="true" name="orgStructures">
      <key column="CLIENT_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoOrgStructure" />
    </set>
    <set inverse="true" cascade="save-update" lazy="true" name="groups">
      <key column="CLIENT_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoGroup" />
    </set>
    <set inverse="true" cascade="save-update" lazy="true" name="permissions">
      <key column="CLIENT_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoAPermissionClient" />
    </set>
    <property name="shortName" />
  </class>
</hibernate-mapping>
