<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoRoleDeputy" name="at.workflow.webdesk.po.model.PoRoleDeputy">
    <cache usage="read-write" />
    <id name="UID" type="string" column="ROLEDEPUTY_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <many-to-one column="PERSON_UID" cascade="none" foreign-key="FK_ROLEDEPUTY_PERSON" lazy="false" not-null="true" name="officeHolder" />
    <bag inverse="true" cascade="none" lazy="true" name="roleHolderPersons">
      <key column="ROLEDEPUTY_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoRoleHolderPerson" />
    </bag>
    <property name="validfrom" not-null="true" type="timestamp" />
    <property name="validto" not-null="true" type="timestamp" />
  </class>
</hibernate-mapping>
