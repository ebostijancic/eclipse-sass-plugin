<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoFieldMapping" name="at.workflow.webdesk.po.model.PoFieldMapping">
    <id name="UID" type="string" column="FIELDMAPPING_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="converter" />
    <property name="converterPattern" />
    <property name="converterLocale" />
    <property name="orderIndicator" />
    <property name="destinationName" />
    <property name="jsFunction" length="2000"/>
    <property name="sourceName" />
    <property name="primaryKey" />
    <many-to-one column="CONNECTORLINK_UID" cascade="none" foreign-key="FK_FIELDMAPPING_CONNECTORLINK" name="connectorLink" />
    <property name="writeBackPrimaryKey" />
    <property name="writeOnInsertOnly" />
  </class>
</hibernate-mapping>
