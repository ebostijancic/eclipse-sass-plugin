<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoJob" name="at.workflow.webdesk.po.model.PoJob">
    <id name="UID" type="string" column="JOB_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="mailForwardingEnabled" />
    <property name="logLevel" />
    <property name="searchCriteria" />
    <property name="linkToLogs" />
    <property name="basicUrl" />
    <property name="emailAdresses" />
    <property name="type" column="jobType" />
    <set inverse="true" lazy="true" name="configFiles">
      <key column="JOB_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoFile" />
    </set>
    <property name="active" />
    <property name="configurable" />
    <property name="jobClass" />
    <property name="name" />
    <property name="description" />
    <set inverse="true" cascade="none" lazy="true" name="jobTriggers">
      <key column="JOB_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoJobTrigger" />
    </set>
    <property name="allowUpdateOnVersionChange" />
    <many-to-one column="JOB_PARENT_UID" cascade="none" foreign-key="FK_JOB_JOB_PARENT" lazy="false" not-null="false" name="parent" />
    <set inverse="true" lazy="true" name="childs">
      <key not-null="false" column="JOB_PARENT_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoJob" />
    </set>
    <many-to-one column="MODULE_UID" cascade="none" foreign-key="FK_JOB_MODULE" lazy="false" not-null="false" name="module" />
  </class>
</hibernate-mapping>
