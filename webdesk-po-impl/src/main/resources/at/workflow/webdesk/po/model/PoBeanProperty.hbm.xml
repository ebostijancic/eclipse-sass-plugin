<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoBeanProperty" name="at.workflow.webdesk.po.model.PoBeanProperty">
    <id name="UID" type="string" column="BEAN_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="password" column="isPassword" />
    <property name="className" />
    <bag inverse="true" cascade="all-delete-orphan" order-by="listIndex asc" lazy="false" name="entries">
      <key column="BEAN_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoBeanPropertyValue" />
    </bag>
    <property name="propertyName" />
    <property name="beanName" />
    <property name="list" column="isList" />
    <many-to-one cascade="none" foreign-key="FK_BEANPROPERTY_MODULE" lazy="false" not-null="false" name="module">
      <column name="MODULE_UID" />
    </many-to-one>
    <property name="type" column="dataType" />
    <property name="detached" />
  </class>
</hibernate-mapping>
