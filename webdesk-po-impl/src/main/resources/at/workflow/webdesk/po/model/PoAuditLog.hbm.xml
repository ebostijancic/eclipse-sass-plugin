<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoAuditLog" name="at.workflow.webdesk.po.model.PoAuditLog">
    <cache usage="read-write" />
    <id name="UID" type="string" column="AUDITLOG_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="attributeName" length="30" />
    <property name="lastModified" type="timestamp" />
    <property name="modifiedBy" length="30" />
    <property name="objectUid" length="32" />
    <property name="objectName" />
    <property name="fieldValue" length="200" />
  </class>
</hibernate-mapping>
