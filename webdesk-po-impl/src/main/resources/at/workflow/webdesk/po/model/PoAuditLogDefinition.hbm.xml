<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoAuditLogDefinition" name="at.workflow.webdesk.po.model.PoAuditLogDefinition">
    <cache usage="read-write" />
    <id name="UID" type="string" column="AUDITLOG_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="objectName" />
    <property name="attributeName" length="30" />
    <property name="dataType" length="30" />
  </class>
</hibernate-mapping>
