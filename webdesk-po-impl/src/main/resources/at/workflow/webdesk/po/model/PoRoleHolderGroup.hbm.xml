<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoRoleHolderGroup" name="at.workflow.webdesk.po.model.PoRoleHolderGroup">
    <cache usage="read-write" />
    <id name="UID" type="string" column="ROLEHOLDERGROUP_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <many-to-one column="ROLEHOLDER_UID" cascade="none" foreign-key="FK_ROLEHOLDERGROUP_ROLEHOLDER" lazy="false" not-null="true" name="roleHolder" />
    <many-to-one column="GROUP_UID" cascade="none" foreign-key="FK_ROLEHOLDERGROUP_GROUP" lazy="false" not-null="true" name="group" />
    <property name="ranking" />
    <property name="validfrom" not-null="true" type="timestamp" />
    <property name="validto" not-null="true" type="timestamp" />
  </class>
</hibernate-mapping>
