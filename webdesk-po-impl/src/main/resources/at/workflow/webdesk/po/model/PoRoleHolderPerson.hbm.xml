<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoRoleHolderPerson" name="at.workflow.webdesk.po.model.PoRoleHolderPerson">
    <cache usage="read-write" />
    <id name="UID" type="string" column="ROLEHOLDERPERSON_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <many-to-one column="ROLEHOLDER_UID" cascade="none" foreign-key="FK_ROLEHOLDERPERSON_ROLEHOLDER" lazy="false" not-null="true" name="roleHolder" />
    <many-to-one column="PERSON_UID" cascade="none" foreign-key="FK_ROLEHOLDERPERSON_PERSON" lazy="false" not-null="true" name="person" />
    <property name="ranking" />
    <many-to-one column="ROLEDEPUTY_UID" cascade="none" foreign-key="FK_ROLEHOLDERPERSON_DEPUTY" lazy="false" not-null="false" name="deputy" />
    <property name="validfrom" not-null="true" type="timestamp" />
    <property name="validto" not-null="true" type="timestamp" />
  </class>
</hibernate-mapping>
