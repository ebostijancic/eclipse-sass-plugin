<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoConnectorLink" name="at.workflow.webdesk.po.model.PoConnectorLink">
    <id name="UID" type="string" column="CONNECTORLINK_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <many-to-one column="DESTCONNECTOR_UID" cascade="none" foreign-key="FK_DSTCONNECTORLINK_CONNECTOR" not-null="true" name="destConnector" />
    <property name="destinationConstraint" length="1000"/>
    <property name="direction" />
    <set inverse="true" cascade="all-delete-orphan" order-by="orderIndicator asc" lazy="true" name="mappingFields">
      <key column="CONNECTORLINK_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoFieldMapping" />
    </set>
    <property name="name" />
    <many-to-one column="SRCCONNECTOR_UID" cascade="none" foreign-key="FK_SRCCONNECTORLINK_CONNECTOR" not-null="true" name="srcConnector" />
    <property name="sourceConstraint"  length="1000"/>
    <property name="syncDeletions" />
    <property name="destinationConnector" />
    <property name="sourceConnector" />
    <property name="converterAfterJavaScript" />
    <property name="convertNullToEmptyString" />
    <property name="locale" column="conversionLocale" />
  </class>
</hibernate-mapping>
