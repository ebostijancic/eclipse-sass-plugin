<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoLanguage" name="at.workflow.webdesk.po.model.PoLanguage">
    <cache usage="read-write" />
    <id name="UID" type="string" column="LANGUAGE_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="code" length="8" unique="true" />
    <property name="name" unique="true" />
    <set inverse="true" cascade="all-delete-orphan" lazy="true" name="textModules">
      <key column="LANGUAGE_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoTextModule" />
    </set>
    <property name="defaultLanguage" length="8" />
    <set inverse="true" cascade="all-delete-orphan" lazy="true" name="helpMessages">
      <key not-null="false" column="LANGUAGE_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoHelpMessage" />
    </set>
  </class>
</hibernate-mapping>
