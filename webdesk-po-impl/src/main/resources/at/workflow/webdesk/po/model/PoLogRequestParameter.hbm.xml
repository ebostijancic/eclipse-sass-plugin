<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoLogRequestParameter" name="at.workflow.webdesk.po.model.PoLogRequestParameter">
    <id name="UID" type="string" column="LOGREQUESTPARAMETER_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="name" />
    <property name="value">
        <column name="theValue" />
    </property>
    <many-to-one column="LOG_UID" cascade="none" foreign-key="FK_LOGREQUESTPARAMETER_LOG" not-null="false" name="log" />
  </class>
</hibernate-mapping>
