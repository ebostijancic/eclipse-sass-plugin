<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoActionCache" name="at.workflow.webdesk.po.model.PoActionCache">
    <cache usage="read-write" />
    <id name="UID" type="string" column="ACTIONCACHE_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="formData" length="16777216" type="org.springframework.orm.hibernate3.support.BlobByteArrayType" />
    <many-to-one cascade="none" foreign-key="FK_ACTIONCACHE_ACTION" not-null="false" name="action">
      <column name="ACTION_UID" unique-key="PERSON_AND_ACTION" />
    </many-to-one>
    <many-to-one cascade="none" foreign-key="FK_ACTIONCACHE_PERSON" not-found="ignore" not-null="false" name="person">
      <column name="PERSON_UID" unique-key="PERSON_AND_ACTION" />
    </many-to-one>
  </class>
</hibernate-mapping>
