<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class lazy="true" table="PoHelpMessage" name="at.workflow.webdesk.po.model.PoHelpMessage">
    <cache usage="read-write" />
    <id name="UID" type="string" column="HELP_MESSAGE_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="text" length="16777216" type="org.springframework.orm.hibernate3.support.BlobByteArrayType" />
    <many-to-one column="ACTION_UID" cascade="none" foreign-key="FK_HELPMESSAGE_ACTION" not-null="true" name="action" />
    <many-to-one column="LANGUAGE_UID" cascade="none" foreign-key="FK_HELPMESSAGE_LANGUAGE" not-null="true" name="language" />
    <property name="allowUpdateOnVersionChange" />
    <property name="url" />
  </class>
</hibernate-mapping>
