<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class lazy="true" table="PoSystemMessage" name="at.workflow.webdesk.po.model.PoSystemMessage">
    <cache usage="read-write" />
    <id name="UID" type="string" column="SYSTEM_MESSAGE_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="becomesValidAt" />
    <property name="url" />
    <property name="createdAt" type="timestamp" />
    <property name="headline" />
    <property name="text" length="16777216" type="org.springframework.orm.hibernate3.support.BlobByteArrayType" />
    <property name="userName" />
  </class>
</hibernate-mapping>
