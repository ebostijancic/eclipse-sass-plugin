<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoLog" name="at.workflow.webdesk.po.model.PoLog">
    <id name="UID" type="string" column="LOG_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="actionName" />
    <property name="beginTime" type="timestamp" />
    <property name="browserVersion" length="40" />
    <property name="ip" length="15" />
    <property name="requestParams" length="2000" />
    <property name="switchedUserName" />
    <property name="userName" />
    <property name="endTime" type="timestamp" />
    <property name="ok" />
    <property name="referenceUID" length="32" />
    <property name="referenceDate" type="timestamp" />
    <property name="referenceId" length="32" />
    <property name="continuationId" length="50" />
    <property name="sessionId" length="32">
      <column name="sessionid" index="IDX_SESSIONID" />
    </property>
    <property name="duration" />
    <property name="ajax"/>
    <property name="url"/>
    <property name="httpMethod" length="10"/>
    <property name="clusterNode" length="32"/>
    
    <bag inverse="true" cascade="all-delete-orphan" lazy="true" name="requestParameters">
      <key column="LOG_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoLogRequestParameter" />
    </bag>
  </class>
</hibernate-mapping>
