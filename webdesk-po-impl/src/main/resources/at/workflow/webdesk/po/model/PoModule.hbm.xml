<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoModule" name="at.workflow.webdesk.po.model.PoModule">
    <cache usage="read-write" />
    <id name="UID" type="string" column="MODULE_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <property name="createdAt" type="timestamp" />
    <property name="name" />
    <property name="detached"/>
    <set inverse="true" cascade="save-update" lazy="true" name="textModules">
      <key column="MODULE_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoTextModule" />
    </set>
    <property name="versionNumber" />
    <set inverse="true" cascade="save-update" lazy="true" name="updateScripts">
      <key column="MODULE_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoFile" />
    </set>
    <set inverse="true" cascade="save-update" lazy="true" name="jobs">
      <key column="MODULE_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoJob" />
    </set>
    <set inverse="true" cascade="save-update" lazy="true" name="connectors">
      <key column="MODULE_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoConnector" />
    </set>
    <set inverse="true" cascade="all-delete-orphan" lazy="true" name="beanProperties">
      <key column="MODULE_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoBeanProperty" />
    </set>
    <set inverse="true" cascade="all-delete-orphan" lazy="true" name="actions">
      <key column="MODULE_UID" />
      <one-to-many class="at.workflow.webdesk.po.model.PoAction" />
    </set>
  </class>
</hibernate-mapping>
