<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PoKeyValue" name="at.workflow.webdesk.po.model.PoKeyValue">
    <cache usage="read-write" />
    <id name="UID" type="string" column="KEYVALUE_UID" length="32">
      <generator class="uuid.hex" />
    </id>
    <many-to-one column="KEYVALUETYPE_UID" cascade="none" foreign-key="FK_KEYVALUE_KEYVALUETYPE" not-null="false" name="keyValueType" />
    <property name="key" length="50" type="string">
      <column name="keyname" />
    </property>
    <property name="filter" column="theFilter" />
    <property name="ranking" />
    <property name="validfrom" not-null="true" type="timestamp" />
    <property name="validto" not-null="true" type="timestamp" />
  </class>
</hibernate-mapping>
